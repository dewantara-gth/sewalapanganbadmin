{
  "name": "yoga/sewalapanganbadmin",
  "description": "A Laravel 12 web app",
  "type": "project",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Yoga\\Sewalapanganbadmin\\": "src/"
    },
    "classmap": [
      "database/"
    ]
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public",
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-install-cmd": [
      "@php artisan migrate --force"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "authors": [
    {
      "name": "MrRen2020",
      "email": "worldlock3@gmail.com"
    }
  ]
}
